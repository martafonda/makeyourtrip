<% if user_signed_in? %>
  <header class="row">
    <%= render partial: 'header_user_logged_in' %>
  </header>
  <section>
    <%= render partial: 'show_city_info', locals: {cities: @cities} %>
  </section>  
<% else %>
  <header>
    <%= render partial: 'header_user_not_logged_in' %>
  </header>
  <section>
    <%= render partial: 'main_content_landing_page' %>
  </section>  
<% end %>